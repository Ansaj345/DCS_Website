* {
  margin: 0px;
  padding: 0px;
}
body {
  font-size: 120%;
  background: #F8F8FF;
}

.header {
  width: 30%;
  margin: 50px auto 0px;
  color: black;
  background: #fd5f00 ;
  text-align: center;
  border: 1px solid #1b1c1e;
  border-bottom: none;
  border-radius: 10px 10px 0px 0px;
  padding: 20px;
}
.form, .content {
  width: 30%;
  margin: 0px auto;
  padding: 20px;
  border: 1px solid black;
  background: transparent;
  border-radius: 0px 0px 10px 10px;
}
.input-group {
  margin: 10px 0px 10px 0px;
}
.input-group label {
  display: block;
  text-align: left;
  margin: 3px;
}
.input-group input {
  height: 30px;
  width: 93%;
  padding: 5px 10px;
  font-size: 16px;
  border-radius: 5px;
  border: 1px solid black;
}
.btn {
  padding: 10px;
  font-size: 15px;
  color: white;
  background: #fd5f00 ;
  border: none;
  border-radius: 5px;
}
.error {
  width: 92%; 
  margin: 0px auto; 
  padding: 10px; 
  border: 2px solid #a94442; 
  color: #a94442; 
  background: #f2dede; 
  border-radius: 5px; 
  text-align: left;
}
.success {
  color: #e65a14; 
  background: #dff0d8; 
  border: 1px solid #eb6709;
  margin-bottom: 20px;
}

.txt2{
  font-size: 20px;
  font-weight: bold;
}

.dropdown .dropbtn {
  font-size: 14px;  
  border: none;
  outline: none;
  color: white ;
  padding: 14px 14px;
  margin-bottom: 0px;
  background-color: inherit;
  font-family: inherit;
  margin: 0;
}
.dropdown-content {
  display: none;
  position: absolute;
  background-color: #0d1128;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(26, 25, 25, 0.2);
  z-index: 1;
}

.dropdown-content a {
  float: none;
  color: white;
  padding: 5px 10px;
  text-decoration: none;
  display: block;
  text-align: left;
}

.dropdown-content a:hover {
  background-color: #0d1128);
}

.dropdown:hover .dropdown-content {
  display: block;
}

.searchform2 {
  height: 54px;
  border: 1px solid rgba(255, 255, 255, 0.1);
  -webkit-border-radius: 30px;
  -moz-border-radius: 30px;
  -ms-border-radius: 30px;
  border-radius: 30px; }
  .searchform2 .form-control {
    width: calc(100% - 52px);
    border: none;
    background: transparent !important;
    color: rgba(255, 255, 255, 0.7) !important;
    font-size: 16px; }
    .searchform2 .form-control::-webkit-input-placeholder {
      /* Chrome/Opera/Safari */
      color: rgba(255, 255, 255, 0.7) !important; }
    .searchform2 .form-control::-moz-placeholder {
      /* Firefox 19+ */
      color: rgba(255, 255, 255, 0.7) !important; }
    .searchform2 .form-control:-ms-input-plac255h255l255er {
      /* IE 0+ */
      color: rgba(255, 255, 255, 0.7) !important; }
    .searchform2 .form-control:-moz-placeholder {
      /* Firefox 18- */
      color: rgba(255, 255, 255, 0.7) !important; }
  .searchform2 .search {
    width: 52px;
    height: 52px; }
    .searchform2 .search span {
      font-size: 20px; }
     
     
      
    
